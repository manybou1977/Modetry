<link href='https://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet'>
{% block stylesheets %}
{{ encore_entry_link_tags('app') }}
{% endblock %}
{# import du css perso la methode asset permet de generér le lien
automatiquement avec twig#}
<link rel="stylesheet" href="{{ asset('assets/css/app.css') }}">
</head>
{% block bodyId %}
{% endblock %}
{% block navbar %}
{% include "./components/navigation.html.twig" %}
{% endblock %}
{# {% include mettre le lien pour inclure la nav bar %} #}
<header>
{% block header %}{% endblock %}
</header>
<body>
    

<main>
{% block body %}{% endblock %}
</main>
{% block bottomReassurance %}{% endblock %}
{% include "./components/footer.html.twig" %}
{# {% include mettre le lien pour inclure le footer %} #}
{# script bootstrap #}<script
src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/js/bootstrap.bundle.m
in.js"
integrity="sha384-qKXV1j0HvMUeCBQ+QVp7JcfGl760yU08IQ+GpUo5hlbpg51QRiuqHAJz8+BrxE/N"
crossorigin="anonymous"> </script>
{# script jsquery #}
<script
src="https://cdn.jsdelivr.net/npm/jquery@3.6.4/dist/jquery.min.js"></script>
{% block javascripts %}
{{ encore_entry_script_tags('app') }}
{% endblock %}
{# Js perso import grace à js #}
<script src="{{ asset('assets/js/app.js') }}"></script>
</body>
</html>